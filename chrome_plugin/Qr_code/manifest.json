  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],/*
  "background":{
   //"page":"background.html",
   "scripts":["jquery-1.10.2.min.js","jquery.qrcode.min.js","bg.js"]
  },*/
  "icons":{
    "19": "image/icon-19.png",
    "38": "image/icon-38.png",
    "128": "image/icon-128.png"
  },
  "browser_action": {
      "default_title": "获取当前路径二维码",
